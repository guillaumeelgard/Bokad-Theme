{
	"name": "Bokad Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#000",
		"editor.foreground": "#bbb",
	},
	"tokenColors": [
		{
			"scope": [
				"text",
			],
			"settings": {
				"foreground": "#eee",
			}
		},
		{
			"scope": [
				"punctuation.definition.entity.html",
				"constant.character.entity",
			],
			"settings": {
				"foreground": "#8ccf78",
			}
		},
		{
			"scope": [
				"support.constant.font-name.css",
				"support.constant.font-name.less",
			],
			"settings": {
				"foreground": "#895091"
			}
		},
		{
			"scope": [
				"storage",
				"keyword.other.type.php",
				"keyword.other.new.php",
				"keyword.other.function.use.php",
				"keyword.control",
				"entity.name.tag",
				"punctuation.definition.tag",
				"support.type",
				"punctuation.support.type.property-name",
			],
			"settings": {
				"foreground": "#b4956b"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html",
				"entity.name.tag.css",
				"entity.name.tag.less",
				"punctuation.definition.entity.css",
				"punctuation.definition.entity.less",
				"punctuation.separator.combinator.css",
				"punctuation.separator.combinator.less",
				"entity.name.tag.wildcard.css",
				"entity.name.tag.wildcard.less",
				"keyword.operator.attribute-selector.css",
				"keyword.operator.attribute-selector.less",
				"meta.selector.css",
				"meta.selector.less",
			],
			"settings": {
				"foreground": "#927041"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"meta.property-value.css",
				"meta.property-value.less",
				"punctuation.definition.constant.css",
				"punctuation.definition.constant.less",
				"keyword.other.important.css",
				"keyword.other.important.less",
				"constant.other.color.rgb-value.css",
				"constant.other.color.rgb-value.less",
				"support.constant.color.w3c-special-color-keyword.css",
				"support.constant.color.w3c-special-color-keyword.less",
				"support.constant.color.w3c-standard-color-name.css",
				"support.constant.color.w3c-standard-color-name.less",
				"support.constant.property-value.css",
				"support.constant.property-value.less",
				"keyword.other.unit.css",
				"keyword.other.unit.less",
				"meta.property-value.css",
				"meta.property-value.less",
				"constant.numeric.css",
				"constant.numeric.less",
			],
			"settings": {
				"foreground": "#b170b9",
			}
		},
		{
			"scope": [
				"variable",
				"punctuation.definition.variable.php",
				"comment.block.documentation.phpdoc.php",
			],
			"settings": {
				"foreground": "#4b74bb"
			}
		},
		{
			"scope": [
				"entity",
				"meta",
				"entity.name.function",
				"meta.function-call",
				"keyword.control.at-rule.charset.css",
				"keyword.control.at-rule.charset.less",
				"punctuation.definition.keyword.css",
				"punctuation.definition.keyword.less",
			],
			"settings": {
				"foreground": "#338f5c"
			}
		},
		{
			"scope": [
				"punctuation",
				"keyword.operator",
				"meta.brace",
				"meta.embedded.block.php",
				"punctuation.separator",
			],
			"settings": {
				"foreground": "#abc6f7"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.other.php",
			],
			"settings": {
				"foreground": "#b83f3f"
			}
		},
		{
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#bdc000"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#666",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"source.sql",
				"keyword.other.sql",
				"constant.character",
				"constant.numeric.sql",
				"storage.modifier.sql",
			],
			"settings": {
				"foreground": "#caa772"
			}
		}
	]
}
